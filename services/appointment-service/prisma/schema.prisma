generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Appointment {
    id        String   @id @default(cuid())
    patientId String
    doctorId  String
    startTime DateTime
    endTime   DateTime
    reason    String?
    status    String   @default("SCHEDULED")
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
